#include <stdio.h>
#include <stdlib.h>
typedef struct
{
char name[30];
int number;
int department name;
int salary;
} 
Employee;
int main()
{
int i, n=20;
Employee employees[n];
printf("Enter %d Employee Details \n \n",n);
for(i=0; i<n; i++)
{
printf("Employee %d:- \n",i+1);
printf("Name: ");
scanf("%s",employees[i].name);
printf("Number: ");
scanf("%d",&employees[i].number);
printf("Department name: ");
scanf("%d",&employees[i].department name);
printf("Salary: ");
scanf("%d",&employees[i].salary);
printf("\n");
}
printf("-------------- All Employees Details ---------------\n");
for(i=0; i<n; i++)
{
printf("Name \t: ");
printf("%s \n",employees[i].name);
printf("Number \t: ");
printf("%d \n",employees[i].number);
printf("Department name \t: ");
printf("%d \n",employees[i].department name);
printf("Salary \t: ");
printf("%d \n",employees[i].salary);
printf("\n");
}
return 0;
}
